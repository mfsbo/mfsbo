{
  "name": "website-vite",
  "version": "1.0.0",
  "description": "mfsbo blog powered by VitePress",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vitepress dev",
    "predev": "node --import tsx/esm ./node_modules/gulp/bin/gulp.js generateMetadata",
    "build": "vitepress build",
    "prebuild": "node --import tsx/esm ./node_modules/gulp/bin/gulp.js generateMetadata",
    "preview": "vitepress preview",
    "gulp": "node --import tsx/esm ./node_modules/gulp/bin/gulp.js",
    "gulp:tasks": "node --import tsx/esm ./node_modules/gulp/bin/gulp.js --tasks"
  },
  "keywords": [
    "blog",
    "vitepress",
    "mfsbo"
  ],
  "author": "mfsbo",
  "license": "ISC",
  "devDependencies": {
    "@types/git-rev-sync": "^2.0.2",
    "@types/gulp": "^4.0.17",
    "@types/gulp-rename": "^2.0.6",
    "@types/gulp-replace": "^0.0.31",
    "@types/node": "^24.6.2",
    "cross-env": "^10.1.0",
    "git-rev-sync": "^3.0.2",
    "gulp": "^5.0.1",
    "gulp-rename": "^2.1.0",
    "gulp-replace": "^1.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitepress": "^1.6.4",
    "vue": "^3.5.20"
  },
  "volta": {
    "node": "24.7.0",
    "npm": "11.5.2"
  },
  "ts-node": {
    "esm": true,
    "transpileOnly": true,
    "require": [
      "tsconfig-paths/register"
    ],
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "NodeNext"
    }
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0"
  }
}
